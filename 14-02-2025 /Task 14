let num1=prompt("Enter the first number:")
num1=Number(num1)
let num2=prompt("Enter the second number:")
num2=Number(num2)
if(num2-num1 ==30)
{
for(let i=num1;i<=num2;i++)
{
if(i%7==0)
break;
console.log(i)
}
}
else 
    console.log("Invalid input! The second number must be 30 more than the first.");
